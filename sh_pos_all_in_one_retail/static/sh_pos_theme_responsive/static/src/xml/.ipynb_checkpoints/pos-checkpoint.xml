	<t t-name="SelectionPopup" t-inherit="point_of_sale.SelectionPopup" t-inherit-mode="extension" owl="1">
		<xpath expr="//div[hasclass('modal-dialog')]" position="replace">
			<div role="dialog" class="modal-dialog">
				<div class="popup popup-selection sh_popup_selection">
					<header class="title">
						<t t-esc="props.title" />
					</header>
					<div class="selection scrollable-y">
						<table class="sh_selection_popup_table" style="width: 100%;">
							<tbody>
								<t t-foreach="props.list" t-as="item" t-key="item.id">
									<tr class="selection-item" t-att-class="{ selected: item.isSelected }" t-on-click="selectItem(item.id)">
										<td>
											<t t-esc="item.label" />
										</td>
									</tr>
								</t>
							</tbody>
						</table>
					</div>
					
					<footer class="footer 78">
						<div class="button cancel" t-on-click="cancel">
							<t t-esc="props.cancelText" />
						</div>
					</footer>
				</div>
			</div>
		</xpath>
	</t>

