    <t t-name="CashInOutPopupWidget" owl="1">
        <div role="dialog" class="modal-dialog sh_cash_in_out_modal_dialog">
            <div class="popup popup-text popup-lg product-configurator-popup cash_control_in_out_popup">
                <header class="title">
                    <t t-if="env.pos.cash_in_out_options">
                    	<t t-if="env.pos.cash_in_out_options == 'cash_in'">
                    		Deposito
                    	</t>
                    	<t t-elif="env.pos.cash_in_out_options == 'cash_out'">
                    		Retiro
                    	</t>
                    </t>
                </header>
                <div class="main-body touch-scrollable scrollable-y main_div_cash_control">
                    <p>
                    	<t t-if="env.pos.cash_in_out_options == 'cash_in'">
                    		Motivo del deposito.
                    	</t>
                    	<t t-elif="env.pos.cash_in_out_options == 'cash_out'">
                    		Motivo del retiro.
                    	</t>
                    </p>
                    <div>
                    	<label class="sh_input_lbl" style="margin-right: 15px;">Motivo : </label> <input type="text" class="reason_input"></input>
                    </div>
                    <div>
                    	<label class="sh_input_lbl">Monto : </label> <input type="text" class="amount_input"></input>
                    </div>
                </div>
                <footer class="footer">
                	<div class="button cash_in_out" t-on-click="cash_in_out">
                        <t t-if="env.pos.cash_in_out_options == 'cash_in'">
                    		Ingreso
                    	</t>
                    	<t t-elif="env.pos.cash_in_out_options == 'cash_out'">
                    		Retiro
                    	</t>
                    </div>
                    <div class="button cancel" t-on-click="cancel">
                        Cancel
                    </div>
                    <div class="button back" t-on-click="back">
                        Atras
                    </div>
                </footer>
            </div>
        </div>
    </t>
    
    <t t-name="SetClosingVBalancePopupWidget" owl="1">
        <div role="dialog" class="modal-dialog sh_cash_in_out_option_modal_dialog">
            <div class="popup popup-text popup-lg product-configurator-popup closing_balance_popup">
                <header class="title">
                    Arqueo de caja
                </header>
                <div class="main-body touch-scrollable scrollable-y main_div_cash_control">
	                <div class="sh_set_closing_body_div">
	                	<table class="sh_set_closing_balance_table">
	                		<thead>
	                			<tr>
	                				<th style="width: 31%;">Cant Bill/Mon</th>
	                				<th style="width: 31%;">Valor Bill/Mon</th>
	                				<th style="width: 30%;">Subtotal</th>
	                				<th style="width: 8%;"></th>
	                			</tr>
	                		</thead>
	                		<tbody>
	                			
	                		</tbody>
	                	</table>
	                </div>
                	<table class="sh_closing_total_table">
                		<tbody>
                			<tr class="sh_closing_total">
                				<td style="width:31%"></td>
                				<td style="width:31%"></td>
                				<td style="width:30%">
                					<span class="closing_total_lbl">
			                			Total :
			                		</span>
			                		<span class="closing_total_data">
			                			0
			                		</span>
                				</td>
                				<td style="width:8%"></td>
                			</tr>
                		</tbody>
                		
                	</table>
                </div>
                <footer class="footer sh_closing_footer">
                	<div class="closing_right_button">
                		<div class="button ok" t-on-click="validate">
	                        Validar
	                    </div>
	                    <div class="button cancel" t-on-click="cancel">
	                        Cancelar
	                    </div>
	                    <div class="button back" t-on-click="back">
	                        Atras
	                    </div>
                	
                		<div class="button add_closing_line" t-on-click="add_closing_line">
	                        Agregar linea
	                    </div>
	                  </div>
	                   
                </footer>
            </div>
        </div>
    </t>
    
    <t t-name="ShConfirmPopup" owl="1">
        <div role="dialog" class="modal-dialog">
            <Draggable>
                <div class="popup popup-confirm sh_popup_confirm">
                    <header class="title drag-handle">
                        <t t-esc="props.title" />
                    </header>
                    <main class="body">
                        <t t-esc=" props.body" />
                    </main>
                    <footer class="footer sh_confirm_footer">
                        <div class="button pos_close" t-on-click="pos_close">
                            Cerrar POS 
                        </div>
<!--
                        <div class="button pos_session_close" t-on-click="pos_session_close">
                            Cerrar session POS
                        </div>
-->

                        <div class="button cancel" t-on-click="cancel">
                            Cancelar
                        </div>
                    </footer>
                </div>
            </Draggable>
        </div>
    </t>
    
    <t t-name="TransactionPopupWidget" owl="1">
        <div role="dialog" class="modal-dialog sh_cash_in_out_option_modal_dialog">
            <div class="popup popup-text popup-lg product-configurator-popup sh_payment_popup">
                <header class="title">
                    Pagos
                </header>
                <div class="main-body touch-scrollable scrollable-y main_div_sh_payment">
                    <div class="main_div_sh_payment_table">
                    	<t t-if="env.pos.db.all_payment and env.pos.db.all_payment.length > 0">
                    		<table class="sh_payment_table">
                    			<thead>
                    				<th style="width: 30%;">Venra</th>
                    				<th style="width: 22%;">Metodo de Pago</th>
                    				<th style="width: 28%;">Monto</th>
                    				<th style="width: 20%;">Fecha</th>
                    			</thead>
                    			<tbody>
                    				<t t-foreach="env.pos.db.all_payment" t-as="each_payment">
                    					<tr>
                    						<td>
                    							<t t-if="each_payment.pos_order_id and each_payment.pos_order_id[1]">
                    								<t t-esc="each_payment.pos_order_id[1]"/>
                    							</t>
                    							<t t-else="">
                    								-
                    							</t>
                    						</td>
                    						<td>
                    							<t t-if="each_payment.payment_method_id and each_payment.payment_method_id[1]">
                    								<t t-esc="each_payment.payment_method_id[1]"/>
                    							</t>
                    							<t t-else="">
                    								-
                    							</t>
                    						</td>
                    						<td>
                    							<t t-if="each_payment.amount">
                    								<t t-esc="env.pos.format_currency(each_payment.amount)"/>
                    							</t>
                    							<t t-else="">
                    								<t t-esc="env.pos.format_currency(0.00)"/>
                    							</t>
                    						</td>
                    						<td>
                    							<t t-if="each_payment.payment_date and each_payment.payment_date.split(' ') and each_payment.payment_date.split(' ')[0]">
                    								<t t-esc="each_payment.payment_date.split(' ')[0]"/>
                    							</t>
                    							<t t-else="">
                    								-
                    							</t>
                    						</td>
                    					</tr>
                    				</t>
                    			</tbody>
                    		</table>
                    	</t>
                    </div>
                    <div class="main_div_sh_payment_footer">
                    	<div class="sh_starting_cash_div">
                    		<label class="starting_cash_lbl sh_lbl">Importe de inicio :</label>
                    		<span class="starting_cash_value sh_value">
                    			<t t-if="env.pos.bank_statement">
                   					<t t-esc="env.pos.format_currency(env.pos.bank_statement.balance_start)"/>
                   				</t>
                   				<t t-else="">
                   					<t t-esc="env.pos.format_currency(0.00)"/>
                   				</t>
                    		</span>
                    	</div>
                    	<div class="sh_transaction_cash_div">
                    		<label class="transaction_cash_lbl sh_lbl">Transacciones :</label>
                    		<span class="transaction_cash_value sh_value">
                    			<t t-if="env.pos.cash_register_total_entry_encoding">
                   					<t t-esc="env.pos.format_currency(env.pos.cash_register_total_entry_encoding)"/>
                   				</t>
                   				<t t-else="">
                   					<t t-esc="env.pos.format_currency(0.00)"/>
                   				</t>
                    		</span>
                    	</div>
                    	<div class="sh_expected_cash_div">
                    		<label class="expected_cash_lbl sh_lbl">Esperado en Efectivo :</label>
                    		<span class="expected_cash_value sh_value">
                    			<t t-esc="env.pos.format_currency(env.pos.bank_statement.balance_start + env.pos.cash_register_total_entry_encoding)"/>
                    		</span>
                    	</div>
                    	<div class="sh_actual_cash_div">
                    		<label class="actual_cash_lbl sh_lbl">Actual en Efectivo :</label>
                    		<span class="actual_cash_value sh_value">
                    			<t t-if="env.pos.cash_register_balance_end_real">
                   					<t t-esc="env.pos.format_currency(env.pos.cash_register_balance_end_real)"/>
                   				</t>
                   				<t t-else="">
                   					<t t-esc="env.pos.format_currency(0.00)"/>
                   				</t>
                    		</span>
                    	</div>
                    </div>
                </div>
                <footer class="footer">
                    <div class="button cancel" t-on-click="cancel">
                        Close
                    </div>
                </footer>
            </div>
        </div>
    </t>
    
    <t t-name="CashInOutOptionStatementPopupWidget" owl="1">
        <div role="dialog" class="modal-dialog sh_cash_in_out_statement_modal_dialog">
            <div class="popup popup-text popup-lg product-configurator-popup sh_cash_in_out_statement_popup">
                <header class="title">
                    Imprimir estado
                </header>
                <div class="main-body touch-scrollable scrollable-y main_div_sh_statement">
                    <div class="main_sub_div_sh_statement">
                    	<div class="sh_statement_option">
                    		<div class="sh_session_div">
	                    		<input class="sh_rd_input" type="radio" id="session_statement" name="statement_option" value="current_session" checked="true"/>
		                        <label class="sh_rd_lbl" for="session_statement">Sesion Actual</label>
		                    </div>
		                    <div class="sh_date_div">
		                        <input class="sh_rd_input" type="radio" id="date_statement" name="statement_option" value="date_wise" />
		                        <label class="sh_rd_lbl" for="date_statement">Rango de fecha</label>
		                    </div>
                    	</div>
                    	<div class="sh_statement_date">
                    		<div class="sh_start_date">
                    			<span class="sh_start_date_lbl">Fecha de inicio: </span>
                    			<span class="sh_start_date_value"><input type="date" class="start_date"/></span>
                    		</div>
                    		<div class="sh_end_date">
                    			<span class="sh_end_date_lbl">Fecha final: </span>
                    			<span class="sh_end_date_value"><input type="date" class="end_date"/></span>
                    		</div>
                    	</div>
                    </div>
                </div>
                <footer class="footer">
                	<div class="sh_print_statement_option">
                		<div class="sh_pdf_div">
	               			<input class="sh_print_input" type="radio" id="pdf_statement" name="print_option" value="pdf" checked="true"/>
	                        <label class="sh_print_lbl" for="pdf_statement">Generar PDF</label>
	                    </div>
	                    <div class="sh_receipt_div">
	                        <input class="sh_print_input" type="radio" id="receipt_statement" name="print_option" value="receipt" />
	                        <label class="sh_print_lbl" for="receipt_statement">Genera recibos</label>
						</div>
                	</div>
                    <div class="button cancel" t-on-click="cancel">
                        Cerrar
                    </div>
                    <div class="button print" t-on-click="print">
                        Imprime
                    </div>
                </footer>
            </div>
        </div>
    </t>
    
</templates>
